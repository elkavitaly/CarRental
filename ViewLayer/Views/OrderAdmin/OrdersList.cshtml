@model System.Collections.Generic.IEnumerable<BusinessLayer.Models.Order>

<table class="users-table">
    <tr>
        <th>OrderId</th>
        <th>User name</th>
        <th>Name</th>
        <th>Class</th>
        <th>Price</th>
        <th>Start date</th>
        <th>End date</th>
        <th>Total</th>
        <th>Status</th>
        <th>Date</th>
        <th></th>
        <th></th>
    </tr>
    @foreach (var order in Model)
    {
        <tr>
            <td>@order.Id</td>
            <td>@order.User.UserName</td>
            <td>@order.Car.Name</td>
            <td>@order.Car.Class</td>
            <td>@order.Car.Price</td>
            <td>@order.Start</td>
            <td>@order.End</td>
            <td>@order.Total</td>
            <td class="status-td">@order.Status</td>
            <td>@order.DateTime</td>
            <td>
                <div class="order-image confirm" value="@order.Id">
                    <img src="https://img.icons8.com/material/24/000000/ok-message.png">
                </div>
            </td>
            <td>
                <div class="order-image decline" value="@order.Id">
                    <img src="https://img.icons8.com/ios/50/000000/multiply-filled.png">
                </div>
            </td>
        </tr>
    }
</table>